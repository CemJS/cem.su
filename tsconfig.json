{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "Cemjsx",
    "jsxFragmentFactory": "CemjsxFragment",
    "module": "ESNext",
    "target": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@images/*": ["./assets/images/*"],
      "@svg/*": ["./assets/svg/*.svg"],
      "@json/*": ["./json/*.json"],
      "@elements/*": ["./elements/*.tsx"]
    }
  },
  "include": ["global.d.ts", "./frontends/**/*", "./elements/**/**/*"]
}
